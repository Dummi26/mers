// 1/n chance to return true
fn random(min int max int) {
    res := run_command("fish" ["-c" "random " + min.to_string() + " " + max.to_string() ...])
    switch res {
        [[]/int string string] res res.1.trim().parse_int().assume1()
    }
}
fn rnd() {
    r := random(5 15)
    switch! r {
        int r r
        [] [] 10
    }
}

words := rnd()
for word regex("\\S+").assume_no_enum().run(text)
    if words > 0 {
        print(word + " ")
        &words = words - 1
        sleep(0.1)
    } else if words == 0 {
        println(word)
        &words = rnd()
        sleep(0.5)
    }
